<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/search_edit_bac">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:orientation="vertical">

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/setting_bac"/>

        <TextView
            android:id="@+id/tvcdTitle"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/padding_margin_10"
            android:layout_marginTop="@dimen/padding_margin_10"
            android:gravity="center"
            android:text="title"
            android:textSize="@dimen/tv_size_15"/>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/home_study"
            android:padding="@dimen/padding_margin_4">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="佣金/税金/合计"
                android:textColor="@color/white"
                android:textSize="@dimen/tv_size_12"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:text="Date"
                android:textColor="@color/white"
                android:textSize="@dimen/tv_size_12"/>


        </RelativeLayout>


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingBottom="@dimen/padding_margin_10"
            android:paddingLeft="@dimen/padding_margin_4"
            android:paddingRight="@dimen/padding_margin_4"
            android:paddingTop="@dimen/padding_margin_10">

            <TextView
                android:id="@+id/tvFirstCom"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="1.0.00/-/-"
                android:textColor="@color/black"
                android:textSize="@dimen/tv_size_10"/>

            <TextView
                android:id="@+id/tvFirstDate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:text="2001/01/01"
                android:textColor="@color/black"
                android:textSize="@dimen/tv_size_10"/>

        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/setting_bac"/>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingBottom="@dimen/padding_margin_10"
            android:paddingLeft="@dimen/padding_margin_4"
            android:paddingRight="@dimen/padding_margin_4"
            android:paddingTop="@dimen/padding_margin_10">

            <TextView
                android:id="@+id/tvSecondtCom"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="1.0.00/-/-"
                android:textColor="@color/black"
                android:textSize="@dimen/tv_size_10"/>

            <TextView
                android:id="@+id/tvSecondtDate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:text="2001/01/01"
                android:textColor="@color/black"
                android:textSize="@dimen/tv_size_10"/>

        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/setting_bac"/>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingBottom="@dimen/padding_margin_10"
            android:paddingLeft="@dimen/padding_margin_4"
            android:paddingRight="@dimen/padding_margin_4"
            android:paddingTop="@dimen/padding_margin_10">

            <TextView
                android:id="@+id/tvThreeCom"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="1.0.00/-/-"
                android:textColor="@color/black"
                android:textSize="@dimen/tv_size_10"/>

            <TextView
                android:id="@+id/tvThreeDate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:text="2001/01/01"
                android:textColor="@color/black"
                android:textSize="@dimen/tv_size_10"/>

        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/setting_bac"/>

    </LinearLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:background="@color/white"
        android:paddingBottom="@dimen/padding_margin_10"
        android:paddingLeft="@dimen/padding_margin_4"
        android:paddingRight="@dimen/padding_margin_4"
        android:paddingTop="@dimen/padding_margin_10">


        <TextView
            android:id="@+id/tvSunCom"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="合计:0.00/0.00/0.00"
            android:textSize="@dimen/tv_size_12"/>


    </RelativeLayout>
</RelativeLayout>