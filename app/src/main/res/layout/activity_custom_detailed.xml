                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="@string/firstname"
                    android:textColor="@color/black"
                    android:textSize="@dimen/two_tv_size"/>

                <ImageView
