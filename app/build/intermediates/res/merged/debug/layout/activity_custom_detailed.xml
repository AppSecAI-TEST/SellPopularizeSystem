<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/MatchMatch"
    android:orientation="vertical">

    <com.yd.org.sellpopularizesystem.myView.BounceScrollView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1">

        <LinearLayout
            android:id="@+id/activity_custom_detailed"
            style="@style/MatchWrap"
            android:orientation="vertical">

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/setting_bac" />

            <RelativeLayout
                style="@style/MatchWrap"
                android:layout_marginLeft="@dimen/padding_margin_10"
                android:padding="@dimen/padding_margin_10">

                <TextView
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="头像"
                    android:textColor="@color/black"
                    android:textSize="@dimen/tv_size_15" />


                <com.yd.org.sellpopularizesystem.myView.CircleImageView
                    android:id="@+id/customeIcon"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_alignParentRight="true"
                    android:src="@mipmap/settingbt" />
                />


            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="@dimen/padding_margin_6"
                android:background="@color/setting_bac" />


            <RelativeLayout
                style="@style/MatchWrap"
                android:layout_marginLeft="@dimen/padding_margin_10"
                android:paddingBottom="@dimen/padding_margin_4"
                android:paddingTop="@dimen/padding_margin_4">

                <TextView
                    android:id="@+id/custmomeDetailedTrueName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="姓"
                    android:textColor="@color/black"
                    android:textSize="@dimen/tv_size_15" />

                <ImageView
                    android:id="@+id/imageTrueName"
                    style="@style/WrapWrap"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/padding_margin_4"
                    android:layout_toRightOf="@id/custmomeDetailedTrueName"
                    android:src="@mipmap/xinghao" />

                <EditText
                    android:id="@+id/edCustomeTrueName"
                    style="@style/WrapWrap"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/padding_margin_4"
                    android:layout_toRightOf="@id/imageTrueName"
                    android:background="@null"
                    android:hint="FamilyName"
                    android:imeOptions="actionNext"
                    android:maxLines="1"
                    android:textColor="@color/gray"
                    android:textColorHint="@color/setting_bac"
                    android:textSize="@dimen/tv_size_15" />

                <LinearLayout
                    style="@style/WrapWrap"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/padding_margin_8"
                    android:layout_toRightOf="@+id/edCustomeTrueName"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/tvFirstName"
                        style="@style/WrapWrap"
                        android:layout_centerVertical="true"
                        android:text="名"
                        android:textColor="@color/black"
                        android:textSize="@dimen/tv_size_15" />

                    <ImageView
                        android:id="@+id/ivFn"
                        style="@style/WrapWrap"
                        android:layout_centerVertical="true"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="@dimen/padding_margin_4"
                        android:layout_toRightOf="@+id/tvFirstName"
                        android:src="@mipmap/xinghao" />

                    <EditText
                        android:id="@+id/etFistName"
                        style="@style/MatchWrap"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/padding_margin_4"
                        android:layout_toRightOf="@+id/ivFn"
                        android:background="@null"
                        android:hint="FistName"
                        android:imeOptions="actionNext"
                        android:maxLines="1"
                        android:textColor="@color/gray"
                        android:textColorHint="@color/setting_bac"
                        android:textSize="@dimen/tv_size_15" />
                </LinearLayout>
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="@dimen/padding_margin_6"
                android:background="@color/setting_bac" />

            <RelativeLayout
                style="@style/MatchWrap"
                android:layout_marginLeft="@dimen/padding_margin_10"
                android:padding="@dimen/padding_margin_4">

                <TextView
                    android:id="@+id/tvEnglishName"
                    android:layout_width="50dp"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="英文名"
                    android:textColor="@color/black"
                    android:textSize="@dimen/tv_size_15" />

                <ImageView
                    android:id="@+id/ivEn"
                    style="@style/WrapWrap"
                    android:layout_centerVertical="true"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="@dimen/padding_margin_4"
                    android:layout_toRightOf="@+id/tvEnglishName"
                    android:src="@mipmap/xinghao" />

                <EditText
                    android:id="@+id/etEnglishName"
                    style="@style/MatchWrap"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/padding_margin_10"
                    android:layout_toRightOf="@+id/ivEn"
                    android:background="@null"
                    android:imeOptions="actionNext"
                    android:maxLines="1"
                    android:padding="@dimen/padding_margin_10"
                    android:textColor="@color/gray"
                    android:textSize="@dimen/tv_size_15" />

            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="@dimen/padding_margin_6"
                android:background="@color/setting_bac" />

            <RelativeLayout
                style="@style/MatchWrap"
                android:layout_marginLeft="@dimen/padding_margin_10"
                android:padding="@dimen/padding_margin_4">

                <TextView
                    android:id="@+id/custmomeDetailedBir"
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="生日"
                    android:textColor="@color/black"
                    android:textSize="@dimen/tv_size_15" />

                <EditText
                    android:id="@+id/edcustmomeDetailedBie"
                    style="@style/MatchWrap"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/padding_margin_10"
                    android:layout_toRightOf="@id/custmomeDetailedBir"
                    android:background="@null"
                    android:focusable="false"
                    android:imeOptions="actionNext"
                    android:maxLines="1"
                    android:padding="@dimen/padding_margin_10"
                    android:textColor="@color/gray"
                    android:textSize="@dimen/tv_size_15" />

            </RelativeLayout>


            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="@dimen/padding_margin_6"
                android:background="@color/setting_bac" />


            <RelativeLayout
                style="@style/MatchWrap"
                android:layout_marginLeft="@dimen/padding_margin_10"
                android:padding="@dimen/padding_margin_4">

                <TextView
                    android:id="@+id/custmomeDetailedMobile"
                    android:layout_width="50dp"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="手机"
                    android:textColor="@color/black"
                    android:textSize="@dimen/tv_size_15" />

                <ImageView
                    android:id="@+id/imageMobile"
                    style="@style/WrapWrap"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/padding_margin_4"
                    android:layout_toRightOf="@id/custmomeDetailedMobile"
                    android:src="@mipmap/xinghao" />

                <EditText
                    android:id="@+id/edCustomeMobile"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/padding_margin_10"
                    android:layout_toLeftOf="@+id/ivPhone"
                    android:layout_toRightOf="@+id/imageMobile"
                    android:background="@null"
                    android:imeOptions="actionNext"
                    android:inputType="phone"
                    android:maxLines="1"
                    android:padding="@dimen/padding_margin_10"
                    android:hint="手机或邮箱二选一"
                    android:gravity="right"
                    android:textColor="@color/gray"
                    android:textSize="@dimen/tv_size_15" />

                <ImageView
                    android:id="@+id/ivPhone"
                    style="@style/WrapWrap"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/padding_margin_10"
                    android:src="@mipmap/phone" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="@dimen/padding_margin_6"
                android:background="@color/setting_bac" />

            <RelativeLayout
                style="@style/MatchWrap"
                android:layout_marginLeft="@dimen/padding_margin_10"
                android:padding="@dimen/padding_margin_4">

                <TextView
                    android:id="@+id/custmomeDetailedEmail"
                    android:layout_width="50dp"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="邮箱"
                    android:textColor="@color/black"
                    android:textSize="@dimen/tv_size_15" />

                <ImageView
                    android:id="@+id/imageEmail"
                    style="@style/WrapWrap"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/padding_margin_4"
                    android:layout_toRightOf="@id/custmomeDetailedEmail"
                    android:src="@mipmap/xinghao" />

                <EditText
                    android:id="@+id/edcustmomeDetailedEmail"
                    style="@style/MatchWrap"
                    android:layout_marginLeft="@dimen/padding_margin_10"
                    android:layout_toLeftOf="@+id/ivEmail"
                    android:layout_toRightOf="@+id/imageEmail"
                    android:background="@null"
                    android:imeOptions="actionNext"
                    android:maxLines="1"
                    android:padding="@dimen/padding_margin_10"
                    android:hint="手机或邮箱二选一"
                    android:gravity="right"
                    android:textColor="@color/gray"
                    android:textSize="@dimen/tv_size_15" />

                <ImageView
                    android:id="@+id/ivEmail"
                    style="@style/WrapWrap"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/padding_margin_6"
                    android:src="@mipmap/email" />

            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="@dimen/padding_margin_6"
                android:background="@color/setting_bac" />

            <RelativeLayout
                style="@style/MatchWrap"
                android:layout_marginLeft="@dimen/padding_margin_10"
                android:padding="@dimen/padding_margin_4">

                <TextView
                    android:id="@+id/custmomeDetailedWeChat"
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="微信"
                    android:textColor="@color/black"
                    android:textSize="@dimen/tv_size_15" />

                <EditText
                    android:id="@+id/edcustmomeDetailedWeChat"
                    style="@style/MatchWrap"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/padding_margin_10"
                    android:layout_toRightOf="@id/custmomeDetailedWeChat"
                    android:background="@null"
                    android:imeOptions="actionNext"
                    android:maxLines="1"
                    android:padding="@dimen/padding_margin_10"
                    android:textColor="@color/gray"
                    android:textSize="@dimen/tv_size_15" />

            </RelativeLayout>


            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="@dimen/padding_margin_6"
                android:background="@color/setting_bac" />


            <RelativeLayout
                style="@style/MatchWrap"
                android:layout_marginLeft="@dimen/padding_margin_10"
                android:padding="@dimen/padding_margin_4">

                <TextView
                    android:id="@+id/custmomeDetailedQQ"
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="QQ号"
                    android:textColor="@color/black"
                    android:textSize="@dimen/tv_size_15" />

                <EditText
                    android:id="@+id/edcustmomeDetailedQQ"
                    style="@style/MatchWrap"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/padding_margin_10"
                    android:layout_toRightOf="@id/custmomeDetailedQQ"
                    android:background="@null"
                    android:imeOptions="actionNext"
                    android:maxLines="1"
                    android:padding="@dimen/padding_margin_10"
                    android:textColor="@color/gray"
                    android:textSize="@dimen/tv_size_15" />

            </RelativeLayout>


            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="@dimen/padding_margin_6"
                android:background="@color/setting_bac" />

            <RelativeLayout
                style="@style/MatchWrap"
                android:layout_marginLeft="@dimen/padding_margin_10"
                android:padding="@dimen/padding_margin_4">

                <TextView
                    android:id="@+id/custmomeDetailedNationality"
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="国籍或地区"
                    android:textColor="@color/black"
                    android:textSize="@dimen/tv_size_15" />

                <EditText
                    android:id="@+id/edcustmomeDetailedNationality"
                    style="@style/MatchWrap"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/padding_margin_10"
                    android:layout_toRightOf="@id/custmomeDetailedNationality"
                    android:background="@null"
                    android:focusable="false"
                    android:imeOptions="actionNext"
                    android:maxLines="1"
                    android:padding="@dimen/padding_margin_10"
                    android:textColor="@color/gray"
                    android:textSize="@dimen/tv_size_15" />

            </RelativeLayout>


            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="@dimen/padding_margin_6"
                android:background="@color/setting_bac" />

            <RelativeLayout
                style="@style/MatchWrap"
                android:layout_marginLeft="@dimen/padding_margin_10"
                android:padding="@dimen/padding_margin_4">

                <TextView
                    android:id="@+id/custmomeDetailedCity"
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="省市区"
                    android:textColor="@color/black"
                    android:textSize="@dimen/tv_size_15" />

                <EditText
                    android:id="@+id/edcustmomeDetailedCity"
                    style="@style/MatchWrap"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentTop="true"
                    android:layout_toRightOf="@+id/custmomeDetailedCity"
                    android:background="@null"
                    android:focusable="false"
                    android:hint="省份 城市 地区"
                    android:imeOptions="actionNext"
                    android:maxLines="1"
                    android:padding="@dimen/padding_margin_10"
                    android:textColor="@color/gray"
                    android:textColorHint="@color/setting_bac"
                    android:textSize="@dimen/tv_size_15" />

            </RelativeLayout>


            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="@dimen/padding_margin_6"
                android:background="@color/setting_bac" />


            <RelativeLayout
                style="@style/MatchWrap"
                android:layout_marginLeft="@dimen/padding_margin_10"
                android:padding="@dimen/padding_margin_4">

                <TextView
                    android:id="@+id/custmomeDetailedAddres"
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="联系地址"
                    android:textColor="@color/black"
                    android:textSize="@dimen/tv_size_15" />

                <EditText
                    android:id="@+id/edcustmomeDetailedAddress"
                    style="@style/MatchWrap"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/padding_margin_10"
                    android:layout_toRightOf="@id/custmomeDetailedAddres"
                    android:background="@null"
                    android:imeOptions="actionNext"
                    android:maxLines="1"
                    android:padding="@dimen/padding_margin_10"
                    android:textColor="@color/gray"
                    android:textSize="@dimen/tv_size_15" />

            </RelativeLayout>


            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="@dimen/padding_margin_6"
                android:background="@color/setting_bac" />


            <RelativeLayout
                style="@style/MatchWrap"
                android:layout_marginLeft="@dimen/padding_margin_10"
                android:padding="@dimen/padding_margin_4">

                <TextView
                    android:id="@+id/tvZipCode"
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="邮编"
                    android:textColor="@color/black"
                    android:textSize="@dimen/tv_size_15" />

                <EditText
                    android:id="@+id/edcustmeZipCode"
                    style="@style/MatchWrap"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/padding_margin_10"
                    android:layout_toRightOf="@id/tvZipCode"
                    android:background="@null"
                    android:imeOptions="actionNext"
                    android:maxLines="1"
                    android:padding="@dimen/padding_margin_10"
                    android:textColor="@color/gray"
                    android:textSize="@dimen/tv_size_15" />

            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="@dimen/padding_margin_6"
                android:background="@color/setting_bac" />

            <RelativeLayout
                style="@style/MatchWrap"
                android:layout_marginLeft="@dimen/padding_margin_10"
                android:padding="@dimen/padding_margin_4">

                <TextView
                    android:id="@+id/tvFirb"
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="FIRB"
                    android:textColor="@color/black"
                    android:textSize="@dimen/tv_size_15" />

                <EditText
                    android:id="@+id/etFirb"
                    style="@style/MatchWrap"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/padding_margin_10"
                    android:layout_toRightOf="@+id/tvFirb"
                    android:background="@null"
                    android:focusable="false"
                    android:gravity="center_vertical|right"
                    android:hint="否"
                    android:imeOptions="actionNext"
                    android:maxLines="1"
                    android:padding="@dimen/padding_margin_10"
                    android:textColor="@color/colorPrimary"
                    android:textColorHint="@color/setting_bac"
                    android:textSize="@dimen/tv_size_15" />

            </RelativeLayout>


            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="@dimen/padding_margin_6"
                android:background="@color/setting_bac" />


            <RelativeLayout
                style="@style/MatchWrap"
                android:layout_marginLeft="@dimen/padding_margin_10"
                android:padding="@dimen/padding_margin_4">

                <TextView
                    android:id="@+id/custmomeDetailedJob"
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="职业"
                    android:textColor="@color/black"
                    android:textSize="@dimen/tv_size_15" />

                <EditText
                    android:id="@+id/edcustmomeDetailedWeJob"
                    style="@style/MatchWrap"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/padding_margin_10"
                    android:layout_toRightOf="@id/custmomeDetailedJob"
                    android:background="@null"
                    android:imeOptions="actionNext"
                    android:maxLines="1"
                    android:padding="@dimen/padding_margin_10"
                    android:textColor="@color/gray"
                    android:textSize="@dimen/tv_size_15" />

            </RelativeLayout>


            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="@dimen/padding_margin_6"
                android:background="@color/setting_bac" />


            <RelativeLayout
                style="@style/MatchWrap"
                android:layout_marginLeft="@dimen/padding_margin_10"
                android:padding="@dimen/padding_margin_4">

                <TextView
                    android:id="@+id/custmomeDetailedSalary"
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="目前年薪"
                    android:textColor="@color/black"
                    android:textSize="@dimen/tv_size_15" />

                <EditText
                    android:id="@+id/edcustmomeDetailedSalary"
                    style="@style/MatchWrap"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/padding_margin_10"
                    android:layout_toRightOf="@id/custmomeDetailedSalary"
                    android:background="@null"
                    android:imeOptions="actionNext"
                    android:maxLines="1"
                    android:padding="@dimen/padding_margin_10"
                    android:textColor="@color/gray"
                    android:textSize="@dimen/tv_size_15" />

            </RelativeLayout>


            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="@dimen/padding_margin_6"
                android:background="@color/setting_bac" />


            <RelativeLayout
                style="@style/MatchWrap"
                android:layout_marginLeft="@dimen/padding_margin_10"
                android:padding="@dimen/padding_margin_4">

                <TextView
                    android:id="@+id/custmomeDetailedCard"
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="身份证"
                    android:textColor="@color/black"
                    android:textSize="@dimen/tv_size_15" />

                <EditText
                    android:id="@+id/edcustmomeDetailedCard"
                    style="@style/MatchWrap"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/padding_margin_10"
                    android:layout_toRightOf="@id/custmomeDetailedCard"
                    android:background="@null"
                    android:imeOptions="actionNext"
                    android:maxLines="1"
                    android:padding="@dimen/padding_margin_10"
                    android:textColor="@color/gray"
                    android:textSize="@dimen/tv_size_15" />

            </RelativeLayout>


            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="@dimen/padding_margin_6"
                android:background="@color/setting_bac" />


            <RelativeLayout
                style="@style/MatchWrap"
                android:layout_marginLeft="@dimen/padding_margin_10"
                android:padding="@dimen/padding_margin_4">

                <TextView
                    android:id="@+id/custmomeDetailedPassPort"
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="护照"
                    android:textColor="@color/black"
                    android:textSize="@dimen/tv_size_15" />

                <EditText
                    android:id="@+id/edcustmomeDetailedPassPort"
                    style="@style/MatchWrap"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/padding_margin_10"
                    android:layout_toRightOf="@id/custmomeDetailedPassPort"
                    android:background="@null"
                    android:imeOptions="actionNext"
                    android:maxLines="1"
                    android:padding="@dimen/padding_margin_10"
                    android:textColor="@color/gray"
                    android:textSize="@dimen/tv_size_15" />

            </RelativeLayout>


            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="@dimen/padding_margin_6"
                android:background="@color/setting_bac" />

            <RelativeLayout
                style="@style/MatchWrap"
                android:layout_marginLeft="@dimen/padding_margin_10"
                android:padding="@dimen/padding_margin_4">

                <TextView
                    android:id="@+id/tvNation"
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="国籍"
                    android:textColor="@color/black"
                    android:textSize="@dimen/tv_size_15" />

                <EditText
                    android:id="@+id/etNation"
                    style="@style/MatchWrap"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/padding_margin_10"
                    android:layout_toRightOf="@id/tvNation"
                    android:background="@null"
                    android:imeOptions="actionNext"
                    android:maxLines="1"
                    android:padding="@dimen/padding_margin_10"
                    android:textColor="@color/gray"
                    android:textSize="@dimen/tv_size_15" />

            </RelativeLayout>


            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="@dimen/padding_margin_6"
                android:background="@color/setting_bac" />

            <RelativeLayout
                style="@style/MatchWrap"
